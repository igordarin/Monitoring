install.packages("sp")

library(sp)
data(meuse)

# Let's see how the meuse dataset is structured:
meuse

# let's look at the first raws of the set
head(meuse)

# let's plot two variables
# let' see if the zin concentration is related to that of copper

attach(meuse)
plot(zinc, copper)
plot(zinc, copper, col= "green")
plot(zinc, copper, col= "green", pch= 19)
plot(zinc, copper, col= "green", pch= 19, cex= 2)
